<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_ACACAC"/>
            <stroke
                android:color="@color/color_ACACAC"
                android:width="0.5dp"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_3B3B3B"/>
            <stroke
                android:color="@color/color_ACACAC"
                android:width="0.5dp"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
</selector>